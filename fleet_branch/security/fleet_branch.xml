<?xml version="1.0" ?>
<openerp>
  <data noupdate="0">

    <record id="fleet_vehicle_modification" model="ir.rule">
      <field name="name">Fleet User may edit only vehicles in his branch</field>
      <field name="model_id" ref="model_fleet_vehicle"/>
      <field name="groups" eval="[(4, ref('fleet.group_fleet_user'))]"/>
      <field name="perm_read" eval="False"/>
      <field name="perm_write" eval="True"/>
      <field name="perm_create" eval="False"/>
      <field name="perm_unlink" eval="False"/>
      <field name="domain_force">[('branch_id', '=', user.branch_id.id)]</field>
    </record>

  </data>
</openerp>
